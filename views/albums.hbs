{{#each datos}}
<div class="card" style="width: 18rem;">
  <img class="card-img-top albums" src="{{this.images.0.url}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text">Canciones {{this.total_tracks}}</p>
    <button type="button" class="btn btn-primary btn-lg"><a href="/tracks/{{this.id}}">View tracks</a></button>
  </div>
</div>
{{/each}}

