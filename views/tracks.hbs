<section class="sectiontracks">
{{#each datos}}
    <div "targettracks">
        <div>
            <h2>{{this.name}}</h2>
        </div>
        <div>
            <audio controls src="{{this.preview_url}}">
                Browser not supported...yet!
                <code>audio</code> element.
            </audio>
        </div>
    </div>
{{/each}}
</section>


